{
  "required_api_version": "^2.0.0",
  "name": "2logseq",
  "description": "Add entries to your logseq inbox or daily file",
  "developer_name": "Alex Qwxlea",
  "icon": "images/logo.png",
  "options": {
    "query_debounce": 0.1
  },
  "preferences": [
    {
      "id": "demo_kw",
      "type": "keyword",
      "name": "logseq",
      "default_value": "l"
    },
    {
      "id": "logseq_path",
      "type": "text",
      "name": "Location of your Logseq Library",
      "description": "Examples: /home/yourusername/Documents/Logseq",
      "default_value": "/home/yourusername/Documents/Logseq"
    },
    {
      "id": "setting_limit",
      "type": "text",
      "name": "Entries",
      "description": "Number of latest entries to show",
      "default_value": "10"
    }
  ]
}
